{
  "name": "pixiv-spider",
  "version": "2.0.0",
  "main": "./src/main.js",
  "scripts": {
    "dev": "npm run browser & npm run electron",
    "browser": "NODE_ENV=development webpack-dev-server --hot --inline --color --host 0.0.0.0 --port 3000",
    "electron": "NODE_ENV=development electron ./src/main.js",
    "test": "NODE_PATH=./src mocha --compilers js:babel-core/register -R spec ",
    "test:watch": "NODE_PATH=./src mocha --compilers js:babel-core/register -R spec --watch",
    "test:cover": "NODE_PATH=./src babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha"
  },
  "dependencies": {
    "cheerio": "^0.19.0",
    "https-proxy-agent": "^1.0.0",
    "image-size": "^0.5.0",
    "isomorphic-fetch": "^2.2.1",
