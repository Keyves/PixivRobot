{
  "name": "PixivRobot",
  "version": "2.0.0",
  "main": "./main/index.js",
  "scripts": {
	"prod": "npm run build && npm run pack:osx",
	"pack:osx": "cp ./package.json ./build && electron-packager ./build PixivRobot --platform=darwin --arch=x64 --out=./prod --overwrite",
	"pack:win": "cp ./package.json ./build && electron-packager ./build PixivRobot --platform=win32 --arch=x64 --out=./prod --overwrite",
	"build": "npm run build:renderer && npm run build:main",
	"build:renderer": "NODE_ENV=production webpack --profile --progress --config ./webpack.config.renderer.js",
	"build:main": "NODE_ENV=production webpack --config ./webpack.config.main.js",
	"dev": "npm run dev:renderer & npm run dev:main",
	"dev:renderer": "NODE_ENV=development webpack-dev-server --hot --inline --color --host 0.0.0.0 --port 3000 --config ./webpack.config.renderer.js",
	"dev:main": "NODE_ENV=development electron -r babel-register ./src/main/index.js",
	"test": "mocha --compilers js:babel-core/register -R spec ",
	"test:watch": "mocha --compilers js:babel-core/register -R spec --watch",
	"test:cover": "babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha"
  },
  "dependencies": {
	"cheerio": "^0.19.0",
	"https-proxy-agent": "^1.0.0",
	"image-size": "^0.5.0",
	"isomorphic-fetch": "^2.2.1",
	"keen-ui": "^0.8.8",
	"material-design-icons": "^2.2.3",
	"vue": "^1.0.24",
	"vue-router": "^0.7.13",
	"vuex": "^0.6.3"
  },
  "devDependencies": {
	"babel-cli": "^6.9.0",
	"babel-core": "^6.8.0",
	"babel-eslint": "^6.0.4",
	"babel-istanbul": "^0.8.0",
	"babel-loader": "^6.2.4",
	"babel-plugin-transform-runtime": "^6.8.0",
	"babel-preset-es2015": "^6.6.0",
	"babel-preset-stage-0": "^6.5.0",
	"babel-register": "^6.9.0",
	"babel-runtime": "^6.6.1",
	"chai": "^3.5.0",
	"css-loader": "^0.23.1",
	"electron-builder": "^4.2.3",
	"electron-packager": "^7.0.3",
	"electron-prebuilt": "^0.37.8",
	"eslint": "^2.9.0",
	"extract-text-webpack-plugin": "^1.0.1",
	"file-loader": "^0.8.5",
	"fs-extra-promise": "^0.3.1",
	"html-webpack-plugin": "^2.10.0",
	"istanbul": "^0.4.3",
	"json-loader": "^0.5.4",
	"less": "^2.7.1",
	"less-loader": "^2.2.3",
	"mocha": "^2.5.3",
	"sinon": "^1.17.4",
	"style-loader": "^0.13.0",
	"url-loader": "^0.5.7",
	"vue-hot-reload-api": "^1.3.2",
	"vue-html-loader": "^1.2.2",
	"vue-loader": "^8.3.1",
	"vue-style-loader": "^1.0.0",
	"webpack": "^1.12.14",
	"webpack-dev-server": "^1.14.1"
  },
  "repository": {
	"type": "git",
	"url": "git+https://github.com/Keyves/pixiv-robot.git"
  },
  "keywords": [
	"electron",
	"pixiv",
	"pixivpicker"
  ],
  "author": "keyves",
  "license": "ISC",
  "bugs": {
	"url": "https://github.com/Keyves/pixiv-robot/issues"
  },
  "homepage": "https://github.com/Keyves/pixiv-robot#readme"
}
